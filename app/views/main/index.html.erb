<script>
var controller;
$(document).ready(function($) {
	var windowHeight;
	// function to be used to retrieve variable
	function getWindowHeight() {
		return windowHeight;
	}	
	// update window height on resize
	$(window).on("resize", function () {
			windowHeight = $(window).innerHeight();
	});		
	// init controller
	controller = new ScrollMagic();
	// build scenes
	new ScrollScene({triggerElement: ".first_section"})
								.setTween(TweenMax.to(".first_man", 3, {autoAlpha: 1, ease: Linear.easeNone}))
								.addTo(controller)		
	new ScrollScene({triggerElement: ".second_section"})
								.setTween(TweenMax.to(".sec_figure", 2, {top: "20", ease: Linear.easeNone}))
								.addTo(controller)
	new ScrollScene({triggerElement: ".third_section"})
								.setTween(TweenMax.from(".third_1_d", 2, {right: "-20", ease: Linear.easeNone}))
